
@model System.Data.DataSet


@{
    ViewBag.Title = "Settings";
    Layout = "~/Views/Shared/_HomeLayout.cshtml";
}

<br/>
<br />
@foreach (System.Data.DataRow row in Model.Tables[0].Rows)
{
  


<div class="row">

    <!-- /.col-lg-8 -->
    <div class="col-lg-10">
        <div class="panel panel-default">
            <div class="panel-heading">
               Warehouse Settings


         
            </div>
            <!-- /.panel-heading -->
            <div class="panel-body">

                <form role="form" class="form-horizontal">
                    <div class="form-group">
                        <label for="wLength" class="col-sm-2 control-label">
                            Warehouse Length (meters)
                        </label>
                        <div class="col-sm-10">
                            <input type="number" min="1" value="@Convert.ToInt32(row["actualWarehouseLength"])" class="form-control-static" style="width:5cm;" id="wLength" placeholder="Length in Meters" required />
                        </div>
                    </div>
                    <div class="form-group">
                        <label for="wWidth" class="col-sm-2 control-label">
                            Warehouse (meters)
                        </label>
                        <div class="col-sm-10">
                            <input type="number" min="1" value="@Convert.ToInt32(row["actualWarehouseWidth"])" class="form-control-static" style="width:5cm;" id="wWidth" placeholder="Width in Meters" required />
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-sm-2">
                        </div>
                        <div class="col-sm-10">
                         

                        </div>
                    </div>
                </form>


            </div>
        </div>
        <!-- /.panel-body -->
    </div>
    <div class="col-lg-10">
        <div class="panel panel-default">
            <div class="panel-heading">
                 Shelve Settings



            </div>
            <!-- /.panel-heading -->
            <div class="panel-body">

                <form role="form" class="form-horizontal">
                    <div class="form-group">
                        <label for="sLength" class="col-sm-2 control-label">
                            Shelve Length (meters)
                        </label>
                        <div class="col-sm-10">
                            <input type="number" min="1" value="@Convert.ToInt32(row["actualShelfLength"])" class="form-control-static" style="width:5cm;" id="sLength" placeholder="Length in Meters" required />
                        </div>
                    </div>
                    <div class="form-group">
                        <label for="sWidth" class="col-sm-2 control-label">
                            Shelve Width (meters)
                        </label>
                        <div class="col-sm-10">
                            <input type="number" min="1" value="@Convert.ToInt32(row["actualShelfWidth"])" class="form-control-static" style="width:5cm;" id="sWidth" placeholder="Width in Meters" required />
                        </div>
                    </div>
                    <div class="form-group">
                        <label for="sHeight" class="col-sm-2 control-label">
                            Shelve Height (meters)
                        </label>
                        <div class="col-sm-10">
                            <input type="number" min="1" value="@Convert.ToInt32(row["actualShelfHeight"])" class="form-control-static" style="width:5cm;" id="sHeight" placeholder="Height in Meters" required />
                        </div>
                    </div>
                    <div class="form-group">
                        <label for="sRows" class="col-sm-2 control-label">
                            Shelve Rows
                        </label>
                        <div class="col-sm-10">
                            <input type="number" min="1" value="@Convert.ToInt32(row["shelfRows"])" class="form-control-static" style="width:5cm;" id="sRows" placeholder="No. of Rows" required />
                        </div>
                    </div>




                    <div class="row">
                        <div class="col-sm-2">
                        </div>
                        <div class="col-sm-10">
                            <div class="tabs-menu">

                                <button id="btnGoBack" class="btn btn-primary">Back</button>
                                                        
                                <button id="btnSaveMe" class="btn btn-primary">Save Changes</button>

                            </div>


                        </div>
                    </div>
                </form>


            </div>
        </div>
        <!-- /.panel-body -->
    </div>


   
</div>
}